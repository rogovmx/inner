<h1>Editing desk_answ</h1>

<% form_for(@desk_answ) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :desk_subj_id %><br />
    <%= f.text_field :subj_id %>
  </p>
  <p>
    <%= f.label :a_text %><br />
    <%= f.text_area :a_text %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @desk_answ %> |
<%= link_to 'Back', desk_answs_path %>
